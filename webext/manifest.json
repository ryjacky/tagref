{
  "manifest_version": 3,
  "name": "TagRef",
  "version": "1.0",
  "permissions": [
    "contextMenus"
  ],
  "action": {
    "default_popup": "popup.html"
  },
  "background": {
    "service_worker": "background.js"
  },
  "host_permissions": ["http://localhost:33728/*"],
  "icons": { 
    "16": "16.png",
    "48": "48.png",
   "128": "128.png" 
  },
  "description": "TagRef Browser Extension"

}